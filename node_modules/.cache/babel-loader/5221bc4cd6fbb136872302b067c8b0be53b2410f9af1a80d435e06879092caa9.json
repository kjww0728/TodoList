   cnt++;\n        }\n      }\n      if (cnt > 0) {\n        this.checkAllOrNot(todoItem, true);\n      } else {\n        this.checkAllOrNot(todoItem, false);\n      }\n    },\n    checkAllOrNot(todoItem, value) {\n      for (var i = 0; i < localStorage.length; i++) {\n        todoItem = JSON.parse(localStorage.getItem(localStorage.key(i)));\n        todoItem.completed = value;\n        localStorage.setItem(todoItem.item, JSON.stringify(todoItem));\n      }\n    },\n    changeFilter(value) {\n      this.filterType = value;\n      console.log(this.filterType);\n    }\n  },\n  components: {\n    'TodoHeader': TodoHeader,\n    'TodoInput': TodoInput,\n    'TodoList': TodoList,\n    'TodoFooter': TodoFooter\n  }\n};","map":{"version":3,"mappings":";AAYA,OAAOA,UAAS,MAAO,yBAAyB;AAChD,OAAOC,SAAQ,MAAO,wBAAwB;AAC9C,OAAOC,QAAO,MAAO,uBAAuB;AAC5C,OAAOC,UAAS,MAAO,yBAAyB;AAEhD,eAAc;EACZC,IAAI,GAAE;IACJ,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,UAAU,EAAE;IACd;EACF,CAAC;EACDC,OAAO,GAAE;IACP,IAAGC,YAAY,CAACC,MAAM,GAAC,CAAC,EAAC;MACvB,KAAI,IAAIC,IAAI,CAAC,EAAEA,CAAC,GAACF,YAAY,CAACC,MAAM,EAAEC,CAAC,EAAE,EAAC;QACxC,IAAGF,YAAY,CAACG,GAAG,CAACD,CAAC,MAAM,6BAA6B,EAAC;UACvD;UACA,IAAI,CAACL,SAAS,CAACO,IAAI,CAACC,IAAI,CAACC,KAAK,CAACN,YAAY,CAACO,OAAO,CAACP,YAAY,CAACG,GAAG,CAACD,CAAC,CAAC,CAAC,CAAC,CAAC;QAC5E;MACF;IACF;EACF,CAAC;EAEDM,OAAO,EAAC;IACNC,OAAO,CAACC,QAAQ,EAAC;MACf,IAAIC,GAAE,GAAI;QAACC,SAAS,EAAC,KAAK;QAAEC,IAAI,EAACH;MAAQ,CAAC;MAC1CV,YAAY,CAACc,OAAO,CAACJ,QAAQ,EAAEL,IAAI,CAACU,SAAS,CAACJ,GAAG,CAAC,CAAC;MACnD,IAAI,CAACd,SAAS,CAACO,IAAI,CAACO,GAAG,CAAC;IAC1B,CAAC;IAEDK,UAAU,CAACN,QAAQ,EAAEO,KAAK,EAAC;MACzBjB,YAAY,CAACkB,UAAU,CAACR,QAAQ,CAACG,IAAI,CAAC;MACtC,IAAI,CAAChB,SAAS,CAACsB,MAAM,CAACF,KAAK,EAAC,CAAC,CAAC;IAChC,CAAC;IAEDG,UAAU,CAACV,QAAQ,EAAC;MAClB,IAAGA,QAAQ,CAACE,SAAQ,KAAM,KAAK,EAAC;QAC9BF,QAAQ,CAACE,SAAQ,GAAI,IAAI;MAC3B,OAAM;QACJF,QAAQ,CAACE,SAAQ,GAAI,KAAK;MAC5B;MACAZ,YAAY,CAACc,OAAO,CAACJ,QAAQ,CAACG,IAAI,EAACR,IAAI,CAACU,SAAS,CAACL,QAAQ,CAAC,CAAC;IAC9D,CAAC;IAEDW,QAAQ,GAAE;MACRrB,Y