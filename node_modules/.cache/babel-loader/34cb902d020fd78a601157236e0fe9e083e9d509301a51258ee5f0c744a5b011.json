ON.stringify(todoItem));\n    },\n\n    clearAll(){\n      localStorage.clear();\n      this.todoItems = [];\n    },\n\n    clearCompleted(){\n      var checkIndex = 0;\n      var checkItem;\n      for(var i = 0; i<localStorage.length; i++){\n        checkItem = JSON.parse(localStorage.getItem(localStorage.key(i)));\n        if(checkItem.completed === true){\n          checkIndex = i;\n          this.removeTodo(checkItem, checkIndex);\n        }\n      }\n    },\n\n    CheckAllTodo(){\n      //for문으로 cnt 세서 false 1이상이면 completed 값을 모두 true로 바꿈\n      // cnt가 0이면 모두 false로 바꿈\n      var cnt = 0;\n      var checkItem;\n      for(var i = 0; i<localStorage.length; i++){\n        checkItem = JSON.parse(localStorage.getItem(localStorage.key(i)));\n        if(checkItem.completed === false){\n        cnt++;\n        }\n      }      \n      if(cnt>0){\n        for(var j = 0; j<localStorage.length; j++){\n        checkItem = JSON.parse(localStorage.getItem(localStorage.key(j)));\n        checkItem.completed = true;\n        localStorage.setItem(checkItem.item,JSON.stringify(checkItem));\n        }\n      }  \n      else{\n        for(var k = 0; k<localStorage.length; k++){\n        checkItem = JSON.parse(localStorage.getItem(localStorage.key(k)));\n        checkItem.completed = false;\n        localStorage.setItem(checkItem.item,JSON.stringify(checkItem));\n        }\n      },\n      showCheckOrNot{\n        \n      }   \n      \n\n    }\n  },\n    components: {\n      'TodoHeader' : TodoHeader,\n      'TodoInput' : TodoInput,\n      'TodoList' : TodoList,\n      'TodoFooter' : TodoFooter\n  }\n}\n</script>\n\n<style>\nhtml {\n\theight: 100%;\n}\n\nbody {\n    display: flex;\n    flex-wrap: nowrap;\n    justify-content: center;\n    background-color: #F5F5F5;\n    min-height: 100%;\n}\n\n.todo-wrapper {\n    justify-content: center;\n    margin-top: 3rem;\n    min-width: 600px;\n}\n\n.todo-box {\n    background-color: white;\n    border: 1px solid #ddd;\n}\n\nbutton {\n    background-color: transparent;\n    border: 0;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}