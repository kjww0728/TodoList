{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport TodoHeader from './components/TodoHeader';\nimport TodoInput from './components/TodoInput';\nimport TodoList from './components/TodoList';\nimport TodoFooter from './components/TodoFooter';\nexport default {\n  data() {\n    return {\n      todoItems: []\n    };\n  },\n  created() {\n    if (localStorage.length > 0) {\n      for (var i = 0; i < localStorage.length; i++) {\n        this.todoItems.push(localStorage.key(i));\n      }\n    }\n  },\n  methods: {\n    addTodo(todoItem) {\n      localStorage.setItem(todoItem, todoItem);\n      this.todoItems.push(todoItem);\n    },\n    removeTodo(todoItem, index) {\n      localStorage.removeItem(todoItem);\n      this.todoItems.splice(index, 1);\n    },\n    clearAll() {\n      localStorage.clear();\n      this.todoItems = [];\n    }\n  },\n  components: {\n    'TodoHeader': TodoHeader,\n    'TodoInput': TodoInput,\n    'TodoList': TodoList,\n    'TodoFooter': TodoFooter\n  }\n};","map":{"version":3,"mappings":";AAUA,OAAOA,UAAS,MAAO,yBAAyB;AAChD,OAAOC,SAAQ,MAAO,wBAAwB;AAC9C,OAAOC,QAAO,MAAO,uBAAuB;AAC5C,OAAOC,UAAS,MAAO,yBAAyB;AAEhD,eAAc;EACZC,IAAI,GAAE;IACJ,OAAO;MACLC,SAAS,EAAE;IACb;EACF,CAAC;EACDC,OAAO,GAAE;IACH,IAAGC,YAAY,CAACC,MAAM,GAAC,CAAC,EAAC;MACrB,KAAI,IAAIC,IAAI,CAAC,EAAEA,CAAC,GAACF,YAAY,CAACC,MAAM,EAAEC,CAAC,EAAE,EAAC;QACtC,IAAI,CAACJ,SAAS,CAACK,IAAI,CAACH,YAAY,CAACI,GAAG,CAACF,CAAC,CAAC,CAAC;MAC5C;IACJ;EACJ,CAAC;EACHG,OAAO,EAAC;IACNC,OAAO,CAACC,QAAQ,EAAC;MACfP,YAAY,CAACQ,OAAO,CAACD,QAAQ,EAAEA,QAAQ,CAAC;MACxC,IAAI,CAACT,SAAS,CAACK,IAAI,CAACI,QAAQ,CAAC;IAC/B,CAAC;IACDE,UAAU,CAACF,QAAQ,EAAEG,KAAK,EAAC;MACzBV,YAAY,CAACW,UAAU,CAACJ,QAAQ,CAAC;MACjC,IAAI,CAACT,SAAS,CAACc,MAAM,CAACF,KAAK,EAAC,CAAC,CAAC;IAChC,CAAC;IACDG,QAAQ,GAAE;MACRb,YAAY,CAACc,KAAK,EAAE;MACpB,IAAI,CAAChB,SAAQ,GAAI,EAAE;IACrB;EACF,CAAC;EACDiB,UAAU,EAAE;IACV,YAAW,EAAItB,UAAU;IACzB,WAAU,EAAIC,SAAS;IACvB,UAAS,EAAIC,QAAQ;IACrB,YAAW,EAAIC;EACjB;AACF","names":["TodoHeader","TodoInput",