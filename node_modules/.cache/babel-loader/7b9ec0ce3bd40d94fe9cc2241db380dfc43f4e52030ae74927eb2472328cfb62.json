{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport TodoHeader from './components/TodoHeader';\nimport TodoInput from './components/TodoInput';\nimport TodoList from './components/TodoList';\nimport TodoFooter from './components/TodoFooter';\nexport default {\n  data() {\n    return {\n      todoItems: []\n    };\n  },\n  created() {\n    if (localStorage.length > 0) {\n      for (var i = 0; i < localStorage.length; i++) {\n        if (localStorage.key(i) !== 'loglevel:webpack-dev-server') {\n          //this.todoItems.push(localStorage.key(i));\n          this.todoItems.push(JSON.parse(localStorage.getItem(localStorage.key(i))));\n        }\n      }\n    }\n  },\n  computed: {\n    CheckAllTodo() {\n      //for문으로 cnt 세서 false 1이상이면 completed 값을 모두 true로 바꿈\n      // cnt가 0이면 모두 false로 바꿈\n      var cnt = 0;\n      if (localStorage.length > 0) {\n        for (var i = 0; i < localStorage.length; i++) {\n          if (localStorage.key(i) !== 'loglevel:webpack-dev-server') {\n            if (this.todoItems[i].completed === false) {\n              cnt += 1;\n            }\n          }\n        }\n      }\n      return cnt;\n    }\n  },\n  methods: {\n    addTodo(todoItem) {\n      var obj = {\n        completed: false,\n        item: todoItem\n      };\n      localStorage.setItem(todoItem, JSON.stringify(obj));\n      this.todoItems.push(obj);\n    },\n    removeTodo(todoItem, index) {\n      localStorage.removeItem(todoItem.item);\n      this.todoItems.splice(index, 1);\n    },\n    toggleTodo(todoItem) {\n      if (todoItem.completed === false) {\n        todoItem.completed = true;\n      } else {\n        todoItem.completed = false;\n      }\n      localStorage.setItem(todoItem.item, JSON.stringify(todoItem));\n    },\n    clearAll() {\n      localStorage.clear();\n      this.todoItems = [];\n    },\n    clearCompleted() {\n      //for문으로 completed값이 true인 값만 삭제 removetodo 함수 그대로 써도 될듯\\\n    }\n  },\n  components: {\n    'TodoHeader': TodoHeader,\n    'TodoInput': TodoInput,\n    'TodoList': TodoList,\n    'TodoFooter': TodoFooter\n  }\n};","map":{"version":3,"mappings":";AAYA,OAAOA,UAAS,MAAO,yBAAyB;AAChD,OAAOC,SAAQ,MAAO,wBAAwB;AAC9C,OAAOC,QAAO,MAAO,uBAAuB;AAC5C,OAAOC,UAAS,MAAO,yBAAyB;AAEhD,eAAc;EACZC,IAAI,GAAE;IACJ,OAAO;MACLC,SAAS,EAAE;IACb;EACF,CAAC;EACDC,OAAO,GAAE;IACP,IAAGC,YAAY,CAACC,MAAM,GAAC,CAAC,EAAC;MACvB,KAAI,IAAIC,IAAI,CAAC,EAAEA,CAAC,GAACF,YAAY,CAACC,MAAM,EAAEC,CAAC,EAAE,EAAC;QACxC,IAAGF,YAAY,CAACG,GAAG,CAACD,CAAC,MAAM,6BAA6B,EAAC;UACvD;UACA,IAAI,CAACJ,SAAS,CAACM,IAAI,CAACC,IAAI,CAACC,KAAK,CAACN,YAAY,CAACO,OAAO,CAACP,YAAY,CAACG,GAAG,CAACD,CAAC,CAAC,CAAC,CAAC,CAAC;QAC5E;MACF;IACF;EACF,CAAC;EACDM,QAAQ,EAAC;IACPC,YAAY,GAAE;MACZ;MACA;MACA,IAAIC,GAAE,GAAI,CAAC;MAEX,IAAGV,YAAY,CAACC,MAAM,GAAC,CAAC,EAAC;QACvB,KAAI,IAAIC,IAAI,CAAC,EAAEA,CAAC,GAACF,YAAY,CAACC,MAAM,EAAEC,CAAC,EAAE,EAAC;UACxC,IAAGF,YAAY,CAACG,GAAG,CAACD,CAAC,MAAM,6BAA6B,EAAC;YACvD,IAAG,IAAI,CAACJ,SAAS,CAACI,CAAC,CAAC,CAACS,SAAQ,KAAM,KAAK,EAAC;cACvCD,GAAG,IAAE,CAAC;YACR;UACF;QACF;MACF;MACA,OAAOA,GAAG;IACZ;EACF,CAAC;EACDE,OAAO,EAAC;IACNC,OAAO,CAACC,QAAQ,EAAC;MACf,IAAIC,GAAE,GAAI;QAACJ,SAAS,EAAC,KAAK;QAAEK,IAAI,EAACF;MAAQ,CAAC;MAC1Cd,YAAY,CAACiB,OAAO,CAACH,QAAQ,EAAET,IAAI,CAACa,SAAS,CAACH,GAAG,CAAC,CAAC;MACnD,IAAI,CAACjB,SAAS,CAACM,IAAI,CAACW,GAAG,CAAC;IAC1B,CAAC;IACDI,UAAU,CAACL,QAAQ,EAAEM,KAAK,EAAC;MACzBpB,YAAY,CAACqB,UAAU,CAACP,QAAQ,CAACE,IAAI,CAAC;MACtC,IAAI,CAAClB,SAAS,CAACwB,MAAM,CAACF,KAAK,EAAC,CAAC,CAAC;IAChC,CAAC;IACDG,UAAU,CAACT,QAAQ,EAAC;MAClB,IAAGA,QAAQ,CAACH,SAAQ,KAAM,KAAK,EAAC;QAC9BG,QAAQ,CAACH,SAAQ,GAAI,IAAI;MAC3B,OAAM;QACJG,QAAQ,CAACH,SAAQ,GAAI,KAAK;MAC5B;MACAX,YAAY,CAACiB,OAAO,CAACH,QAAQ,CAACE,IAAI,EAACX,IAAI,CAACa,SAAS,CAACJ,QAAQ,CAAC,CAAC;IAC9D,CAAC;IACDU,QAAQ,GAAE;MACRxB,YAAY,CAACyB,KAAK,EAAE;MACpB,IAAI,CAAC3B,SAAQ,GAAI,EAAE;IACrB,CAAC;IACD4B,cAAc,GAAE;MACd;IAAA;EAGJ,CAAC;EACCC,UAAU,EAAE;IACV,YAAW,EAAIlC,UAAU;IACzB,WAAU,EAAIC,SAAS;IACvB,UAAS,EAAIC,QAAQ;IACrB,YAAW,EAAIC;EACnB;AACF","names":["TodoHeader","TodoInput","TodoL