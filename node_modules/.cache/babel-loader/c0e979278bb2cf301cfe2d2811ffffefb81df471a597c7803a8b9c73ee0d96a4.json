{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport TodoHeader from './components/TodoHeader';\nimport TodoInput from './components/TodoInput';\nimport TodoList from './components/TodoList';\nimport TodoFooter from './components/TodoFooter';\nexport default {\n  data() {\n    return {\n      todoItems: [],\n      filterType: 'All'\n    };\n  },\n  created() {\n    if (localStorage.length > 0) {\n      for (var i = 0; i < localStorage.length; i++) {\n        if (localStorage.key(i) !== 'loglevel:webpack-dev-server') {\n          //this.todoItems.push(localStorage.key(i));\n          this.todoItems.push(JSON.parse(localStorage.getItem(localStorage.key(i))));\n        }\n      }\n    }\n  },\n  computed: {},\n  methods: {\n    addTodo(todoItem) {\n      var obj = {\n        completed: false,\n        item: todoItem\n      };\n      localStorage.setItem(todoItem, JSON.stringify(obj));\n      this.todoItems.push(obj);\n    },\n    removeTodo(todoItem, index) {\n      localStorage.removeItem(todoItem.item);\n      this.todoItems.splice(index, 1);\n    },\n    toggleTodo(todoItem) {\n      if (todoItem.completed === false) {\n        todoItem.completed = true;\n      } else {\n        todoItem.completed = false;\n      }\n      localStorage.setItem(todoItem.item, JSON.stringify(todoItem));\n    },\n    clearAll() {\n      localStorage.clear();\n      this.todoItems = [];\n    },\n    clearCompleted() {\n      var checkIndex = 0;\n      var checkItem;\n      for (var i = 0; i < localStorage.length; i++) {\n        checkItem = JSON.parse(localStorage.getItem(localStorage.key(i)));\n        if (checkItem.completed === true) {\n          checkIndex = i;\n          this.removeTodo(checkItem, checkIndex);\n        }\n      }\n    },\n    checkAllTodo() {\n      //for문으로 cnt 세서 false 1이상이면 completed 값을 모두 true로 바꿈\n      // cnt가 0이면 모두 false로 바꿈\n      var cnt = 0;\n      var checkItem;\n      for (var i = 0; i < localStorage.length; i++) {\n        checkItem = JSON.parse(localStorage.getItem(localStorage.key(i)));\n        if (checkItem.completed === false) {\n          cnt++;\n        }\n      }\n      // if(cnt>0){\n      //   for(var j = 0; j<localStorage.length; j++){\n      //   checkItem = JSON.parse(localStorage.getItem(localStorage.key(j)));\n      //   checkItem.completed = true;\n      //   localStorage.setItem(checkItem.item,JSON.stringify(checkItem));\n      //   }\n      // }  \n      // else{\n      //   for(var k = 0; k<localStorage.length; k++){\n      //   checkItem = JSON.parse(localStorage.getItem(localStorage.key(k)));\n      //   checkItem.completed = false;\n      //   localStorage.setItem(checkItem.item,JSON.stringify(checkItem));\n      //   }\n      // }\n    },\n\n    showCheckOrNot(chheckItem, value) {\n      for (var i = 0; i < localStorage.length; i++) {\n        checkItem = JSON.parse(localStorage.getItem(localStorage.key(i)));\n        checkItem.completed = value;\n        localStorage.setItem(checkItem.item, JSON.stringify(checkItem));\n      }\n    }\n  },\n  components: {\n    'TodoHeader': TodoHeader,\n    'TodoInput': TodoInput,\n    'TodoList': TodoList,\n    'TodoFooter': TodoFooter\n  }\n};","map":{"version":3,"mappings":";AAYA,OAAOA,UAAS,MAAO,yBAAyB;AAChD,OAAOC,SAAQ,MAAO,wBAAwB;AAC9C,OAAOC,QAAO,MAAO,uBAAuB;AAC5C,OAAOC,UAAS,MAAO,yBAAyB;AAEhD,eAAc;EACZC,IAAI,GAAE;IACJ,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,UAAS,EAAG;IACd;EACF,CAAC;EACDC,OAAO,GAAE;IACP,IAAGC,YAAY,CAACC,MAAM,GAAC,CAAC,EAAC;MACvB,KAAI,IAAIC,IAAI,CAAC,EAAEA,CAAC,GAACF,YAAY,CAACC,MAAM,EAAEC,CAAC,EAAE,EAAC;QACxC,IAAGF,YAAY,CAACG,GAAG,CAACD,CAAC,MAAM,6BAA6B,EAAC;UACvD;UACA,IAAI,CAACL,SAAS,CAACO,IAAI,CAACC,IAAI,CAACC,KAAK,CAACN,YAAY,CAACO,OAAO,CAACP,YAAY,CAACG,GAAG,CAACD,CAAC,CAAC,CAAC,CAAC,CAAC;QAC5E;MACF;IACF;EACF,CAAC;EACDM,QAAQ,EAAC,CAET,CAAC;EACDC,OAAO,EAAC;IACNC,OAAO,CAACC,QAAQ,EAAC;MACf,IAAIC,GAAE,GAAI;QAACC,SAAS,EAAC,KAAK;QAAEC,IAAI,EAACH;MAAQ,CAAC;MAC1CX,YAAY,CAACe,OAAO,CAACJ,QAAQ,EAAEN,IAAI,CAACW,SAAS,CAACJ,GAAG,CAAC,CA